# Suppress source map warnings from third-party libraries
GENERATE_SOURCEMAP=false

# Backend API Base URL
# Development: http://localhost:4000
# Production: https://api.yourdomain.com
REACT_APP_API_BASE_URL=http://localhost:4000

# File Upload Configuration
# Maximum file size in MB (applies to document uploads and verification)
REACT_APP_MAX_FILE_SIZE_MB=5

# Application Branding
REACT_APP_NAME=PapDocAuthX
REACT_APP_SHORT_NAME=PDX+

# Contact & Support URLs
REACT_APP_SECURITY_EMAIL=security@yourdomain.com
REACT_APP_STATUS_URL=https://status.yourdomain.com
REACT_APP_TRUST_URL=https://trust.yourdomain.com
